{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-extra-semi": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-unexpected-multiline": "off",
    "no-unused-vars": "warn",
    "eqeqeq": "off"
  }
}
